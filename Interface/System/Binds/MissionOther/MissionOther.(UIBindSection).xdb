<?xml version="1.0" encoding="UTF-8" ?>
<UIBindSection>
	<sysName>mission_other</sysName>
	<name href="Section.txt" />
	<binds>
		<Item>
			<sysName>toggle_ui</sysName>
			<name href="ToggleUi.txt" />
			<defaultKeys>
				<Item>ALT+Z</Item>
			</defaultKeys>
		</Item>
		<Item>
			<sysName>toggle_minimized_ui</sysName>
			<name href="ToggleMinimizedUi.txt" />
			<defaultKeys>
				<Item>SHIFT+ALT+Z</Item>
			</defaultKeys>
		</Item>
		<Item>
			<sysName>make_screenshot</sysName>
			<name href="MakeScreenshot.txt" />
			<defaultKeys>
				<Item>SYSRQ</Item>
			</defaultKeys>
		</Item>
		<Item>
			<sysName>highlight_objects</sysName>
			<name href="HighlightObjects.txt" />
			<activationType>ACTIVATION_TYPE_ACTIVATE_ONLY</activationType>
			<defaultKeys>
				<Item>T</Item>
			</defaultKeys>
		</Item>
		<Item>
			<sysName>chat_talk_reply</sysName>
			<name href="ChatTalkReply.txt" />
			<defaultKeys>
				<Item>BACKSPACE</Item>
			</defaultKeys>
		</Item>
		<Item>
			<sysName>toggle_inspect_target</sysName>
			<name href="ToggleInspectTarget.txt" />
			<defaultKeys>
				<Item>CTRL+I</Item>
			</defaultKeys>
		</Item>
	</binds>
</UIBindSection>
