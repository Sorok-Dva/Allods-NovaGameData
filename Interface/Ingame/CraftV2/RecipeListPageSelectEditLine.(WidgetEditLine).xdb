<?xml version="1.0" encoding="UTF-8" ?>
<WidgetEditLine>
	<Header>
		<Prototype href="/Interface/Common/EditLine/Prototype.(WidgetEditLine).xdb#xpointer(/WidgetEditLine)" />
	</Header>
	<Name>PageSelect</Name>
	<Placement>
		<X>
			<Align>WIDGET_ALIGN_CENTER</Align>
			<Size>32</Size>
		</X>
		<Y>
			<Align>WIDGET_ALIGN_BOTH</Align>
			<Pos>6</Pos>
			<HighPos>6</HighPos>
		</Y>
	</Placement>
	<IgnoreDblClick>true</IgnoreDblClick>
	<ReactionEsc>pageselect_esc</ReactionEsc>
	<ReactionEnter>pageselect_enter</ReactionEnter>
	<ReactionChanged>pageselect_changed</ReactionChanged>
	<TextStyle>
		<wrapText>false</wrapText>
		<showClippedSymbol>true</showClippedSymbol>
		<ellipsis>false</ellipsis>
		<Align>ALIGNY_MIDDLE</Align>
	</TextStyle>
	<maxSymbolsCount>3</maxSymbolsCount>
	<filterAlias>NUMBERS</filterAlias>
	<globalClassName>EditLineGlobalCenter</globalClassName>
</WidgetEditLine>
