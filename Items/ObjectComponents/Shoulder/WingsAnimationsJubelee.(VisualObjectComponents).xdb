<?xml version="1.0" encoding="UTF-8" ?>
<VisualObjectComponents>
	<components>
		<Item type="AnimationFromParentComponent">
			<VisualObjectComponentID />
			<controller href="/Characters/Upper.(AnimationController).xdb#xpointer(/AnimationController)" />
			<synchronizeAnimationPosition>false</synchronizeAnimationPosition>
			<animationsReplacements>
				<Item>
					<play>jump</play>
					<forAnimations>
						<Item>jump</Item>
						<Item>jumpEnd</Item>
						<Item>jumpLandRun</Item>
						<Item>mountRideJumpLandRun</Item>
						<Item>mountRideJump</Item>
						<Item>fall</Item>
					</forAnimations>
				</Item>
				<Item>
					<play>jumpStart</play>
					<forAnimations>
						<Item>mountRideJumpStart</Item>
						<Item>jumpStart</Item>
					</forAnimations>
				</Item>
				<Item>
					<play>run</play>
					<forAnimations>
						<Item>run</Item>
						<Item>run01</Item>
						<Item>run02</Item>
						<Item>run1H</Item>
						<Item>run2H</Item>
						<Item>run2HL</Item>
						<Item>runBow</Item>
						<Item>runCharge</Item>
						<Item>runSpellCharged</Item>
						<Item>runSpellChargedWand</Item>
					</forAnimations>
				</Item>
			</animationsReplacements>
			<ifAnimationNotExistsRun>idle</ifAnimationNotExistsRun>
		</Item>
	</components>
</VisualObjectComponents>
