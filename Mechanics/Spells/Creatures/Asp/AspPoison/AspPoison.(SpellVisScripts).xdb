			<Item>
				<locator>Slot_Mouth</locator>
				<effectFx href="/Spells/FX/Spells/ReadySpellDirectPoison.(VisObjectTemplate).xdb#xpointer(/VisObjectTemplate)" />
			</Item>
		</visualEffects>
	</wholeCasting>
</SpellVisScripts>
