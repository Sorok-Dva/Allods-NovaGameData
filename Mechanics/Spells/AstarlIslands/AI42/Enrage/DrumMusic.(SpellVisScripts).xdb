<?xml version="1.0" encoding="UTF-8" ?>
<SpellVisScripts>
	<wholeCasting type="VisActionList">
		<play>Simultaneously</play>
		<playWhile type="CreatureAnimationAction">
			<mode>LOOP</mode>
			<animations>
				<Item>emotePlayMusic</Item>
			</animations>
			<channel>CHANNEL_ABILITY</channel>
			<channelOverrideMode>OVERRIDE_IGNORE</channelOverrideMode>
			<channelOverrideControllers>TOP_AND_BOTTOM</channelOverrideControllers>
			<advanced>
				<turnToTarget>NO_TURN</turnToTarget>
			</advanced>
		</playWhile>
		<elements>
			<Item type="Sound2DAction">
				<sound>
					<project href="/SFX/Music/Music.(FMODProject).xdb#xpointer(/FMODProject)" />
					<name>Music/Ingame/RacesMusicInstrumrnts</name>
				</sound>
				<actionType>MusicSource</actionType>
				<musicSourceMaxRadius>50</musicSourceMaxRadius>
				<musicSourceParameters>
					<Item>
						<parameter href="/Client/SlashCommands/MusicalInstruments/Orc.(SoundParameter).xdb#xpointer(/SoundParameter)" />
						<value>1</value>
					</Item>
				</musicSourceParameters>
			</Item>
		</elements>
	</wholeCasting>
	<precastType>PRECAST_ACTIONBARSTART</precastType>
</SpellVisScripts>
