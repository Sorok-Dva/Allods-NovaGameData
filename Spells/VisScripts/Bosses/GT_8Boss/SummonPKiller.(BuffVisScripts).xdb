<?xml version="1.0" encoding="UTF-8" ?>
<BuffVisScripts>
	<action type="VisActionList">
		<play>Simultaneously</play>
		<elements>
			<Item type="VisActionList">
				<visActionID />
				<play>InSequence</play>
				<playWhile />
				<elements>
					<Item type="CreatureWaitReadyAction">
						<visActionID />
					</Item>
					<Item type="CreatureEffectsAction">
						<visActionID />
						<visualEffects>
							<Item>
								<delay>0</delay>
								<effectFx href="/Spells/FX/Spells/MageDisplacement.(VisObjectTemplate).xdb#xpointer(/VisObjectTemplate)" />
								<effectAlias href="" />
								<effectAnimations>
									<mode>DIE</mode>
								</effectAnimations>
								<fixPoint>true</fixPoint>
								<lockScale>false</lockScale>
								<scale>1</scale>
								<fadeInTime>0</fadeInTime>
								<fadeOutTime>0</fadeOutTime>
								<locator>Slot_BodyFX</locator>
								<member>Default_Member</member>
								<locatorName />
								<offendTime />
							</Item>
						</visualEffects>
						<stopOnDeath>false</stopOnDeath>
					</Item>
				</elements>
				<stopOnDeath>false</stopOnDeath>
			</Item>
			<Item type="VisActionList">
				<elements>
					<Item type="CreatureAnimationAction">
						<visActionID />
						<mode>DIE</mode>
						<use>USE_ANIMATIONS</use>
						<animations>
							<Item>jump</Item>
						</animations>
						<channel>CHANNEL_ABILITY</channel>
						<channelOverrideMode>OVERRIDE_DEFAULT</channelOverrideMode>
						<channelOverrideScript />
						<channelOverrideControllers>DEFAULT_CONTROLLERS</channelOverrideControllers>
						<specialController />
						<advanced>
							<speed>0</speed>
							<backgroundAfterFirstKey>false</backgroundAfterFirstKey>
							<changeChannelAfterFirstKey>NOT_CHANGE</changeChannelAfterFirstKey>
							<channelAfterFirstKey>CHANNEL_RETURNING</channelAfterFirstKey>
							<offendType>NO_OFFEND</offendType>
							<stopIfRotatedOrMoved>false</stopIfRotatedOrMoved>
							<autoHasten>
								<mode>AUTOHASTEN_NONE</mode>
								<timeToBeLeft>0</timeToBeLeft>
							</autoHasten>
							<eventPlaybackEnd>false</eventPlaybackEnd>
							<turnToTarget>TURN_DEFAULT</turnToTarget>
							<waitRealEvent>WAIT_DEFAULT</waitRealEvent>
						</advanced>
					</Item>
					<Item type="CreatureAnimationAction">
						<visActionID />
						<mode>DIE</mode>
						<use>USE_ANIMATIONS</use>
						<animations>
							<Item>jumpEnd</Item>
						</animations>
						<channel>CHANNEL_EMOTE</channel>
						<channelOverrideMode>OVERRIDE_DEFAULT</channelOverrideMode>
						<channelOverrideScript />
						<channelOverrideControllers>DEFAULT_CONTROLLERS</channelOverrideControllers>
						<specialController />
						<advanced>
							<speed>0</speed>
							<backgroundAfterFirstKey>false</backgroundAfterFirstKey>
							<changeChannelAfterFirstKey>NOT_CHANGE</changeChannelAfterFirstKey>
							<channelAfterFirstKey>CHANNEL_RETURNING</channelAfterFirstKey>
							<offendType>NO_OFFEND</offendType>
							<stopIfRotatedOrMoved>false</stopIfRotatedOrMoved>
							<autoHasten>
								<mode>AUTOHASTEN_NONE</mode>
								<timeToBeLeft>0</timeToBeLeft>
							</autoHasten>
							<eventPlaybackEnd>false</eventPlaybackEnd>
							<turnToTarget>TURN_DEFAULT</turnToTarget>
							<waitRealEvent>WAIT_DEFAULT</waitRealEvent>
						</advanced>
					</Item>
				</elements>
			</Item>
		</elements>
	</action>
</BuffVisScripts>
