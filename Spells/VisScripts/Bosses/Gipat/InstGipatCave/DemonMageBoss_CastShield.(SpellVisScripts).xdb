<?xml version="1.0" encoding="UTF-8" ?>
<SpellVisScripts>
	<Header>
		<Prototype href="/Spells/VisScripts/Prototypes2/Mob.(SpellVisScripts).xdb#xpointer(/SpellVisScripts)" />
		<Variables>
			<AnimationPreCastMode>LOOP</AnimationPreCastMode>
			<AnimationPreCast>readySpell</AnimationPreCast>
			<FxOnHands />
			<FxOnCaster />
			<Animation>spellCastOmni</Animation>
		</Variables>
	</Header>
	<actionBarStart type="VisActionList">
		<elements>
			<Item skiped="" />
			<Item type="CreatureAnimationAction">
				<animations>
					<Item var="AnimationPreCast" />
				</animations>
			</Item>
		</elements>
	</actionBarStart>
	<wholeCasting type="VisActionList">
		<elements>
			<Item type="CreatureEffectsAction">
				<visualEffects>
					<Item>
						<effectFx hrefvar="FxOnHands" />
						<locator>Slot_BodyFX</locator>
					</Item>
					<Item>
						<effectFx hrefvar="FxOnHands" />
					</Item>
					<Item>
						<effectFx href="/Spells/FX/Spells/ReadySpellSimple.(VisObjectTemplate).xdb#xpointer(/VisObjectTemplate)" />
						<locator>Slot_BodyFX</locator>
					</Item>
					<Item>
						<delay>0</delay>
						<effectFx href="/Spells/FX/Mobs/DemonCrabBoss_ReadySpellDirectDown.(VisObjectTemplate).xdb#xpointer(/VisObjectTemplate)" />
						<effectAlias href="" />
						<effectAnimations>
							<mode>DIE</mode>
						</effectAnimations>
						<fixPoint>false</fixPoint>
						<lockScale>false</lockScale>
						<scale>2</scale>
						<fadeInTime>0</fadeInTime>
						<fadeOutTime>0</fadeOutTime>
						<locator>Slot_Global</locator>
						<member>Default_Member</member>
						<locatorName />
						<offendTime />
					</Item>
				</visualEffects>
			</Item>
			<Item />
		</elements>
	</wholeCasting>
	<launch type="VisActionList">
		<elements>
			<Item type="CreatureAnimationAction">
				<animations>
					<Item var="Animation" />
				</animations>
			</Item>
			<Item skiped="" />
		</elements>
	</launch>
</SpellVisScripts>
