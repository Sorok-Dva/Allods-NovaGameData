						<effectAnimations>
							<mode>DIE</mode>
						</effectAnimations>
						<fixPoint>false</fixPoint>
						<lockScale>false</lockScale>
						<scale>1</scale>
						<fadeInTime>0</fadeInTime>
						<fadeOutTime>0</fadeOutTime>
						<locator>Slot_Global</locator>
						<member>Default_Member</member>
						<locatorName />
