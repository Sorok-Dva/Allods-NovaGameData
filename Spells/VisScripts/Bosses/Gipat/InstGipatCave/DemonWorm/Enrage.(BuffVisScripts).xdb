					</autoHasten>
					<eventPlaybackEnd>false</eventPlaybackEnd>
					<turnToTarget>TURN_DEFAULT</turnToTarget>
					<waitRealEvent>WAIT_DEFAULT</waitRealEvent>
				</advanced>
			</Item>
			<Item type="CreatureEffectsAction">
				<visActionID />
				<visualEffects>
					<Item>
						<delay>0</delay>
						<effectFx href="/Spells/FX/Mobs/OutDoorBoss_Enrage.(VisObjectTemplate).xdb#xpointer(/VisObjectTemplate)" />
