<?xml version="1.0" encoding="UTF-8" ?>
<BuffVisScripts>
	<action />
	<postAction type="VisActionList">
		<visActionID />
		<play>Simultaneously</play>
		<playWhile />
		<elements>
			<Item type="VisActionList">
				<visActionID />
