<?xml version="1.0" encoding="UTF-8" ?>
<SpellVisScripts>
	<Header>
		<Prototype href="MountGryphon_Jump.(SpellVisScripts).xdb#xpointer(/SpellVisScripts)" />
	</Header>
	<launch type="VisActionList">
		<visActionID />
		<play>Simultaneously</play>
		<playWhile type="VisActionDelay">
			<visActionID />
			<time>2000</time>
		</playWhile>
		<elements>
			<Item type="CreatureEffectsAction">
				<visualEffects>
					<Item>
						<effectAnimations>
							<animations />
						</effectAnimations>
					</Item>
				</visualEffects>
				<restartOnVisCreatureChange>false</restartOnVisCreatureChange>
			</Item>
			<Item type="Sound3DAction">
				<visActionID />
				<sound>
					<project href="/SFX/Items/Items.(FMODProject).xdb#xpointer(/FMODProject)" />
					<name>Items/PvPWing/Jump</name>
				</sound>
				<alias href="" />
				<forcedSoundStop>false</forcedSoundStop>
			</Item>
		</elements>
		<stopOnDeath>false</stopOnDeath>
		<stopWhileWhenElementsEnded>true</stopWhileWhenElementsEnded>
	</launch>
</SpellVisScripts>
