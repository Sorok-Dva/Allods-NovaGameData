<?xml version="1.0" encoding="UTF-8" ?>
<SpellVisScripts>
	<wholeCasting type="VisActionList">
		<play>Simultaneously</play>
		<playWhile type="VisActionDelay">
			<time>2500</time>
		</playWhile>
		<elements>
			<Item type="CreatureReplaceAnimationsAction">
				<onTop>
					<animationsReplacements>
						<Item>
							<play>specialAttack</play>
							<forAnimations>
								<Item>jumpEnd</Item>
							</forAnimations>
						</Item>
						<Item>
							<play>special01</play>
							<forAnimations>
								<Item>jumpStart</Item>
							</forAnimations>
						</Item>
						<Item>
							<play>special02</play>
							<forAnimations>
								<Item>jump</Item>
							</forAnimations>
						</Item>
					</animationsReplacements>
				</onTop>
				<onBottom>
					<animationsReplacements>
						<Item>
							<play>specialAttack</play>
							<forAnimations>
								<Item>jumpEnd</Item>
							</forAnimations>
						</Item>
						<Item>
							<play>special01</play>
							<forAnimations>
								<Item>jumpStart</Item>
							</forAnimations>
						</Item>
						<Item>
							<play>special02</play>
							<forAnimations>
								<Item>jump</Item>
							</forAnimations>
						</Item>
					</animationsReplacements>
				</onBottom>
				<member>Mount</member>
				<restartOnVisualChange>true</restartOnVisualChange>
			</Item>
			<Item type="CreatureEffectsAction">
				<visualEffects>
					<Item>
						<effectFx href="/Spells/FX/Spells/Maze_KHD_Haste01_Trail.(VisObjectTemplate).xdb#xpointer(/VisObjectTemplate)" />
						<locator>Slot_FX</locator>
					</Item>
				</visualEffects>
			</Item>
			<Item type="CreatureDisableHitsAction" />
			<Item type="Sound3DAction">
				<sound>
					<project href="/SFX/Spells/Sounds.(FMODProject).xdb#xpointer(/FMODProject)" />
					<name>spells/Mount/MountTitan/Jump</name>
				</sound>
			</Item>
		</elements>
	</wholeCasting>
	<launch />
</SpellVisScripts>
