<?xml version="1.0" encoding="UTF-8" ?>
<BuffVisScripts>
	<Header>
		<isPrototype>true</isPrototype>
		<Variables>
			<Animation>emoteDance</Animation>
			<FxEffect />
			<Locator />
			<LocatorName />
			<FadeInTime>200</FadeInTime>
			<FadeOutTime>500</FadeOutTime>
		</Variables>
	</Header>
	<action type="VisActionList">
		<play>Simultaneously</play>
		<elements>
			<Item type="CreatureEffectsAction">
				<visualEffects>
					<Item>
						<locator var="Locator" />
						<locatorName var="LocatorName" />
						<effectFx hrefvar="FxEffect" />
						<fadeInTime var="FadeInTime" />
						<fadeOutTime var="FadeOutTime" />
					</Item>
				</visualEffects>
			</Item>
			<Item type="CreatureAnimationAction">
				<mode>LOOP</mode>
				<use>USE_ANIMATIONS</use>
				<animations>
					<Item var="Animation" />
				</animations>
				<channel>CHANNEL_KNOCKDOWN</channel>
				<advanced>
					<backgroundAfterFirstKey>false</backgroundAfterFirstKey>
				</advanced>
			</Item>
		</elements>
	</action>
</BuffVisScripts>
